{
  "parserOptions": {
    "ecmaVersion": "6",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-set-state": "off",
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}